{
  "name": "multi-static-example",
  "version": "1.0.0",
  "author": "Avin Lambrero",
  "license": "MIT",
  "scripts": {
    "dev": "node ./node_modules/multi-static/dev.js",
    "build": "node ./node_modules/multi-static/build.js",
    "postinstall": "node ./postinstall.js",
    "dev:site1": "npm run dev -- --config ./configs/site1.config.js",
    "dev:site2": "npm run dev -- --config ./configs/site2.config.js",
    "build:site1": "npm run build -- --config ./configs/site1.config.js",
    "build:site2": "npm run build -- --config ./configs/site2.config.js"
  },
  "dependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "7.8.4",
    "autoprefixer": "9.7.6",
    "babel-loader": "^8.2.2",
    "localhost-certs": "1.0.0",
    "mocker-api": "1.11.2",
    "multi-static": "link:..",
    "mustache": "4.2.0",
    "node-sass": "4.12.0",
    "postcss": "7.0.27",
    "promise-polyfill": "^8.2.0",
    "read-first-line": "1.0.4",
    "static-hash-version": "1.0.0",
    "terser-webpack-plugin": "2.3.4",
    "webpack": "4.41.5",
    "webpack-dev-middleware": "3.7.2"
  }
}
